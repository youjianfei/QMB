<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context="com.jingnuo.quanmb.activity.Mytaskdetails_JinxingzhongActivity">
    <include
        android:id="@+id/re_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_title"
        />
    <TextView
        android:id="@+id/textview_titleee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="师傅已接单，正在赶来"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="10dp"
        android:layout_below="@+id/re_title"
        />
    <RelativeLayout
        android:id="@+id/ree1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@mipmap/background_matchtask"
        android:layout_below="@+id/textview_titleee"
        >
        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/image_head"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="40dp"
            android:src="@mipmap/user_pic" />
        <TextView
            android:id="@+id/text_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:layout_toRightOf="@+id/image_head"
            android:layout_marginTop="38dp"
            android:layout_marginLeft="15dp"
            android:lines="1"
            android:textSize="20dp"
            android:text="名字" />
        <TextView
            android:id="@+id/text_vip"
            android:layout_width="60dp"
            android:layout_height="20dp"
            android:layout_toRightOf="@+id/text_name"
            android:layout_marginTop="42dp"
            android:layout_marginLeft="15dp"
            android:text="会员商家"
            android:paddingTop="2dp"
            android:paddingBottom="2dp"
            android:paddingLeft="4dp"
            android:paddingRight="4dp"
            android:textSize="10dp"
            android:textColor="@color/white"
            android:background="@drawable/button_red_20dp"
            android:gravity="center"
            android:visibility="invisible"
            />

        <com.jingnuo.quanmb.customview.SimpleRatingBar
            android:id="@+id/simpleRatingBar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srb_isIndicator="true"
            app:srb_starSize="16dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"
            android:layout_below="@+id/text_name"
            android:layout_toRightOf="@+id/image_head"
            />

        <TextView
            android:id="@+id/text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/text_orders"
            android:layout_toRightOf="@+id/simpleRatingBar"
            android:lines="1"
            android:text="|"
            android:layout_marginLeft="10dp"
            android:textColor="@color/gray_background"
            android:textSize="13dp" />
        <TextView
            android:id="@+id/text_orders"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/yellow_jianbian_end"
            android:layout_below="@+id/text_name"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="20dp"
            android:lines="1"
            android:textSize="13dp"
            android:layout_toRightOf="@+id/simpleRatingBar"
            android:text="0单" />

        <ImageView
            android:id="@+id/image"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/gray_background"
            android:layout_marginLeft="55dp"
            android:layout_marginRight="55dp"
            android:layout_below="@+id/image_head"
            android:layout_marginTop="20dp"
            />
        <TextView
            android:id="@+id/textview_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="用户评价"
            android:textSize="12dp"
            android:layout_below="@+id/image"
            android:layout_marginLeft="55dp"
            android:layout_marginTop="15dp"
            android:textColor="@color/text_hide_tip"
            />
        <GridView
            android:id="@+id/gridview_userthink"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:numColumns="2"
            android:layout_below="@+id/textview_1"
            android:layout_marginLeft="45dp"
            android:layout_marginRight="45dp"
            android:horizontalSpacing="15dp"
            android:verticalSpacing="15dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="20dp"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/ree1"
        >
        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:src="@mipmap/resttime"
            android:layout_marginLeft="38dp"
            android:layout_marginTop="2dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="预约时间"
            android:layout_marginLeft="68dp"
            android:textSize="15dp"
            />
        <TextView
            android:id="@+id/textview_yuyuetime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="尽快"
            android:layout_alignParentRight="true"
            android:layout_marginRight="33dp"
            android:textSize="15dp"
            android:textColor="@color/black"
            />

        <ImageView
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:src="@mipmap/address_blue"
            android:layout_marginLeft="38dp"
            android:layout_marginTop="27dp"
            android:layout_below="@+id/textview_yuyuetime"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="地址"
            android:layout_marginLeft="68dp"
            android:textSize="15dp"
            android:layout_below="@+id/textview_yuyuetime"
            android:layout_marginTop="25dp"
            />
        <TextView
            android:id="@+id/textview_yuyueaddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="工作地点"
            android:layout_alignParentRight="true"
            android:layout_marginRight="33dp"
            android:textSize="15dp"
            android:textColor="@color/black"
            android:layout_below="@+id/textview_yuyuetime"
            android:layout_marginTop="25dp"
            />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/relativelayout1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:background="@mipmap/background_line">
        <LinearLayout
            android:id="@+id/linearLayout_cancle"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_centerVertical="true"
                android:layout_marginTop="5dp"
                android:src="@mipmap/chexiao1" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="撤销"
                android:layout_marginTop="5dp"
                android:textColor="@color/gray_969696"
                android:layout_marginLeft="5dp"
                android:textSize="13dp" />
        </LinearLayout>
        <ImageView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/gray_background"
            android:layout_marginTop="10dp"
            />
        <LinearLayout
            android:id="@+id/linearlayout_zixun"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/linearLayout_callphone"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_centerVertical="true"
                android:layout_marginTop="5dp"
                android:src="@mipmap/kefu3" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                >
                <TextView
                    android:id="@+id/qqq"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="客服介入"
                    android:textSize="13dp"
                    android:layout_marginLeft="15dp"
                    android:textColor="@color/gray_969696"
                    android:visibility="visible"
                    android:gravity="center"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="服务有疑问？"
                    android:textSize="11dp"
                    android:layout_marginLeft="15dp"
                    android:textColor="@color/gray_969696"
                    android:visibility="visible"
                    android:gravity="center"
                    android:layout_below="@+id/qqq"
                    />
            </RelativeLayout>
        </LinearLayout>
        <ImageView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:background="@color/gray_background"
            />
        <LinearLayout
            android:id="@+id/linearLayout_callphone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginTop="5dp"
                android:layout_centerVertical="true"
                android:src="@mipmap/dianhua" />

            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="打电话"
                android:layout_marginLeft="8dp"
                android:textColor="@color/gray_969696"
                android:textSize="13dp" />
        </LinearLayout>


    </LinearLayout>

</RelativeLayout>
