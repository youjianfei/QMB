<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_background"
    tools:context="com.jingnuo.quanmb.activity.SettingActivity">
    <include
        android:id="@+id/re_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/include_title"
        />

    <TextView
        android:id="@+id/textview_cleancache"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="清除缓存"
        android:layout_marginTop="1dp"
        android:layout_below="@+id/re_title"
        android:textColor="@color/black_text"
        android:paddingLeft="20dp"
        android:background="@color/white"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        />
    <TextView
        android:id="@+id/textview_cleancacheSize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0Kb"
        android:layout_marginTop="1dp"
        android:layout_below="@+id/re_title"
        android:textColor="@color/black_text"
        android:paddingLeft="20dp"
        android:background="@color/white"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="20dp"
        />
    <!--<ImageView-->
        <!--android:layout_width="15dp"-->
        <!--android:layout_height="15dp"-->
        <!--android:src="@mipmap/more"-->
        <!--android:layout_below="@+id/textview_suggest"-->
        <!--android:layout_alignParentRight="true"-->
        <!--android:layout_marginRight="20dp"-->
        <!--android:layout_marginTop="16dp"-->
        <!--android:id="@+id/imageView12" />-->
    <TextView
        android:id="@+id/textview_shareAPP"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="分享APP"
        android:layout_marginTop="1dp"
        android:layout_below="@+id/textview_cleancache"
        android:textColor="@color/black_text"
        android:paddingLeft="20dp"
        android:background="@color/white"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        />
    <ImageView
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:src="@mipmap/more"
        android:layout_below="@+id/textview_cleancache"
        android:layout_alignParentRight="true"
        android:layout_marginRight="20dp"
        android:layout_marginTop="16dp"
        android:id="@+id/imageView" />

    <TextView
        android:id="@+id/textview_aboutus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="关于我们"
        android:layout_marginTop="1dp"
        android:layout_below="@+id/textview_shareAPP"
        android:textColor="@color/black_text"
        android:paddingLeft="20dp"
        android:background="@color/white"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        />
    <ImageView
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:src="@mipmap/more"
        android:layout_below="@+id/textview_shareAPP"
        android:layout_alignParentRight="true"
        android:layout_marginRight="20dp"
        android:layout_marginTop="16dp"
         />
    <TextView
        android:id="@+id/textview_logout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="退出登录"
        android:textSize="15dp"
        android:textColor="@color/text_title2"
        android:gravity="center"
        android:layout_alignParentBottom="true"
        android:padding="5dp"
        android:background="@color/white"
        />



</RelativeLayout>
