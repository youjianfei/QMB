            <!--/>-->

        <TextView
            android:id="@+id/text_main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/SimpleRatingBar"
            android:layout_marginTop="35dp"
            android:layout_marginLeft="50dp"
            android:layout_marginRight="40dp"
            android:layout_marginBottom="5dp"
            android:textColor="@color/black"
            android:maxLines="2"
            android:lineSpacingMultiplier="1.2"
            android:textSize="14dp"
            android:text="主营：" />


        <LinearLayout
            android:id="@+id/linearlayout__"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/image_head"
            android:layout_marginLeft="35dp"
            android:layout_marginRight="25dp"
            android:layout_marginTop="100dp"
            android:gravity="center_vertical"
            >
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_weight="1">
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/zizhi"
                    android:layout_centerVertical="true"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="资质认证"
                    android:textSize="12dp"
                    android:layout_marginLeft="5dp"
                    android:textColor="@color/gray_969696"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_weight="1">
                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:src="@mipmap/jiaoyi"
                    android:layout_centerVertical="true"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="交易保障"
                    android:textSize="12dp"
                    android:layout_marginLeft="5dp"
                    android:textColor="@color/gray_969696"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_weight="1">

                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:layout_centerVertical="true"

                    android:src="@mipmap/shouhou"
                    android:textSize="12dp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="售后保障"
                    android:textSize="12dp"
                    android:layout_marginLeft="5dp"
                    android:textColor="@color/gray_969696"
                    />
            </LinearLayout>

        </LinearLayout>


